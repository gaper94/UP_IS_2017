#include <iostream>

using namespace std;

// Podtochka a)
int max(int a, int b)
{
    return a > b ? a : b;
}

// Podtochka b)
int max(int a, int b, int c, int d, int e)
{
    return max(max(max(a, b), max(c, d)), e);
}

int main()
{
    cout << "Max of 1 and 6 is " << max(1, 6) << endl;
    cout << "Max of 1, 4, 2, 10, -1 is " << max(1, 4, 2, 10, -1) << endl;
}
